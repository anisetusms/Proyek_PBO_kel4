<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="kel4.DataBookingAdminController">
    <children>
        <Label text="Data Booking" layoutX="20.0" layoutY="20.0" style="-fx-font-size: 18px; -fx-font-weight: bold;" />

        <TableView fx:id="tableView" layoutX="20.0" layoutY="60.0" prefHeight="300.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="colNama" text="Nama" prefWidth="100.0" />
                <TableColumn fx:id="colNamaLapangan" text="Nama Lapangan" prefWidth="120.0" />
                <TableColumn fx:id="colTanggalBooking" text="Tanggal Booking" prefWidth="120.0" />
                <TableColumn fx:id="colJamMulai" text="Jam Mulai" prefWidth="80.0" />
                <TableColumn fx:id="colJamSelesai" text="Jam Selesai" prefWidth="80.0" />
                <TableColumn fx:id="colStatus" text="Status" prefWidth="100.0" />
            </columns>
        </TableView>

        <Button fx:id="btnTerima" text="Terima" layoutX="650.0" layoutY="100.0" prefWidth="100.0"
                onAction="#handleTerima" />
        <Button fx:id="btnTolak" text="Tolak" layoutX="650.0" layoutY="150.0" prefWidth="100.0"
                onAction="#handleTolak" />
        <Button fx:id="btnKembali" text="Kembali" layoutX="650.0" layoutY="300.0" prefWidth="100.0"
                onAction="#handleKembali" />
    </children>
</AnchorPane>
